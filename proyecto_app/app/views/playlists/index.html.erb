

<body style="background-color: #333; color: white">


<p id="notice"><%= notice %></p>

<h1>Playlists</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @playlists.each do |playlist| %>
      <tr>
        <td><%= playlist.name %></td>
        <td><%= link_to 'Show', playlist %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

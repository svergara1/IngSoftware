<style>
body, html {
    height: 100%;
    color: white;
}
</style>
<p id="notice"><%= notice %></p>
<body background="http://wallpapers-best.com/uploads/posts/2015-10/11_black_jaguar.jpg">
<h1>Users</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
